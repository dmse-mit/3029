 CellChangeTimes->{{3.85397417376716*^9, 
  3.8539741878592377`*^9}},ExpressionUUID->"35d26b33-f2e6-4648-a007-\
a0050bdc91d0"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{"v", "==", 
    FractionBox["p", 
