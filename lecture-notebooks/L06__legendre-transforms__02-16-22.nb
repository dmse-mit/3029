Cell["\<\
Aha, here\[CloseCurlyQuote]s the culprit! \[LineSeparator]Two branches - we\
\[CloseCurlyQuote]ll pick the positive one\
\>", "Item",
 CellChangeTimes->{{3.853974195337043*^9, 
  3.8539742239798117`*^9}},ExpressionUUID->"be6e6777-e23b-4447-8919-\
d2a8a9cf688c"],

Cell[CellGroupData[{

Cell["We now use our definition for the Legendre transform", "Item",
 CellChangeTimes->{{3.8539742688228817`*^9, 
  3.8539742785235233`*^9}},ExpressionUUID->"5e499ca4-5678-4562-8bd1-\
3e90ef4ec8fe"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"p", "=", 
     FractionBox[
      RowBox[{"m", " ", "v"}], 
      SqrtBox[
       RowBox[{"1", "-", 
        SuperscriptBox["v", "2"]}]]]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{"Simplify", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"p", " ", "v"}], " ", "-", " ", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["m", "2"], "+", 
        SuperscriptBox["p", "2"]}]]}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"m", ">", "0"}], ",", 
       RowBox[{
        SuperscriptBox["v", "2"], "<", "1"}]}], "}"}]}], "]"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 CellChangeTimes->{{3.8539742323401957`*^9, 3.8539742569958*^9}},
