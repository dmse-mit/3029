Cell[BoxData[
 RowBox[{
  RowBox[{"visualizeLattice", "[", "state_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"posA", ",", "posB", ",", "posV"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"posV", ",", "posA", ",", "posB"}], "}"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Position", "[", 
         RowBox[{"state", ",", "#", ",", 
          RowBox[{"{", "2", "}"}], ",", 
          RowBox[{"Heads", "\[Rule]", "False"}]}], "]"}], "&"}], "/@", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1", ",", "2"}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", 
      RowBox[{
       RowBox[{"MapThread", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"EdgeForm", "[", "Black", "]"}], ",", 
            RowBox[{"FaceForm", "[", "#1", "]"}], ",", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"Disk", "[", 
               RowBox[{
                RowBox[{"Reverse", "[", "i", "]"}], ",", 
                RowBox[{"1", "/", "2"}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "#2"}], "}"}]}], "]"}]}], "}"}], "&"}], ",", 
         
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"White", ",", "Red", ",", "Blue"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"posV", ",", "posA", ",", "posB"}], "}"}]}], "}"}]}], 
        "]"}], "\[IndentingNewLine]", ",", 
       RowBox[{"PlotRangePadding", "\[Rule]", "None"}], ",", 
       RowBox[{"ImageSize", "\[Rule]", "500"}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.859841399572461*^9, 3.859841647871745*^9}, {
  3.8598430616513042`*^9, 3.859843065163281*^9}},
