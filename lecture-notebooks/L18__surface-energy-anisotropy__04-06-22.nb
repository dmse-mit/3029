Cell[CellGroupData[{

Cell["\<\
We wish to isolate the orientation-dependent part of the surface energy, by \
subtracting the bulk energy and surface energy contributions from the round \
part of the surface\
\>", "Item",
 CellChangeTimes->{{3.8580263373637857`*^9, 
  3.85802640458997*^9}},ExpressionUUID->"7350059c-bf53-43fd-ad02-\
45d1b584bbe3"],

Cell[CellGroupData[{

Cell["\<\
To do so we will compute the average LJ energy for the two cleaved halves and \
fit it to half the energy model we used earlier, plus a\
\>", "Subitem",
 CellChangeTimes->{{3.8580263373637857`*^9, 3.858026478262845*^9}, {
  3.8580276530099697`*^9, 3.858027677966914*^9}, {3.858027710003743*^9, 
  3.858027711322123*^9}, {3.858028163084816*^9, 
  3.858028163309944*^9}},ExpressionUUID->"ae9d9f0d-beb1-44ca-b6e7-\
ba5a341c83a0"],

Cell[BoxData[
 RowBox[{
  RowBox[{"cleavedParticleEnergyModel", "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      RowBox[{"bulkEnergy", " ", 
       RowBox[{"(", 
        RowBox[{"Pi", " ", 
         SuperscriptBox["radius", "2"]}], ")"}]}], " ", "+", " ", 
      RowBox[{"\[Gamma]", " ", 
       RowBox[{"(", 
        RowBox[{"2", " ", "Pi", " ", "radius"}], ")"}]}]}], "2"], "+", 
    RowBox[{"2", " ", "\[Sigma]", " ", "radius"}], "+", 
    RowBox[{"2", "\[Rho]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8580266396698*^9, 3.858026652566101*^9}, {
  3.858026692785863*^9, 3.858026724738793*^9}},
 CellLabel->"In[12]:=",ExpressionUUID->"2afe51e2-736f-44b7-a304-bf11967ef177"],

Cell[BoxData[
 RowBox[{
  RowBox[{"cleavedParticles", " ", "=", " ", 
   RowBox[{"Association", "@", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"\[Theta]", "->", 
       RowBox[{"Association", "@", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"radius", "->", 
           RowBox[{
            RowBox[{"cleaveParticle", "[", "radius", "]"}], "[", "\[Theta]", 
            "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"radius", ",", 
            RowBox[{"Range", "[", 
             RowBox[{"5", ",", "50", ",", "5"}], "]"}]}], "}"}]}], "]"}]}]}], 
      ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "0", ",", 
        RowBox[{"2", " ", "Pi"}], ",", 
        RowBox[{"Pi", "/", "36"}]}], "}"}]}], "]"}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.858026745774016*^9, 3.858026746759273*^9}, {
  3.8580267851424713`*^9, 3.8580267862829237`*^9}, {3.858026908906362*^9, 
  3.8580269834237843`*^9}, {3.858027121622876*^9, 3.8580271506654987`*^9}},
 CellLabel->"In[13]:=",ExpressionUUID->"e7a21193-98ab-4792-8ece-10825519acc1"],

Cell[BoxData[{
 RowBox[{"Clear", "[", "anisotropicFit", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"anisotropicFit", "[", "\[Theta]_", "]"}], ":=", 
  RowBox[{
   RowBox[{"anisotropicFit", "[", "\[Theta]", "]"}], "=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"data", ",", "fit"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"data", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"r", ",", 
           RowBox[{"Mean", "[", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"aggregatedLennardJonesEnergies", "/@", 
               RowBox[{
                RowBox[{"cleavedParticles", "[", "\[Theta]", "]"}], "[", "r", 
                "]"}]}], ")"}], "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", "\"\<Total Energy\>\""}], "]"}], "]"}], 
            "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", "5", ",", "50", ",", "5"}], "}"}]}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"fit", "=", 
       RowBox[{"FindFit", "[", 
        RowBox[{"data", ",", "cleavedParticleEnergyModel", ",", 
         RowBox[{"{", 
          RowBox[{
          "\[Sigma]", ",", "\[Rho]", ",", "bulkEnergy", ",", "\[Gamma]"}], 
          "}"}], ",", "radius"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"<|", 
       RowBox[{
        RowBox[{"\"\<\[Theta]\>\"", "->", "\[Theta]"}], ",", 
        RowBox[{
         RowBox[{"\"\<\[Sigma]\>\"", "->", " ", "\[Sigma]"}], "/.", "fit"}], 
        ",", 
        RowBox[{
         RowBox[{"\"\<\[Rho]\>\"", "->", " ", "\[Rho]"}], "/.", "fit"}]}], 
       "|>"}]}]}], "\[IndentingNewLine]", "]"}]}]}]}], "Input",
 CellChangeTimes->{{3.858027346935338*^9, 3.858027411507332*^9}, {
   3.858027457599028*^9, 3.8580274586672363`*^9}, {3.858028250569043*^9, 
