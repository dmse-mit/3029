      RowBox[{"period", " ", "=", 
       RowBox[{"Last", "[", 
        RowBox[{"Last", "[", 
         RowBox[{"InterpolatingFunctionDomain", "[", "xSols", "]"}], "]"}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"<|", 
       RowBox[{
        RowBox[{"\"\<Domain\>\"", "\[Rule]", " ", "domainLength"}], ",", 
        RowBox[{"\"\<Period\>\"", "\[Rule]", " ", "period"}], ",", 
        RowBox[{"\"\<Mode\>\"", "\[Rule]", " ", "mode"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"\"\<Positions\>\"", "\[Rule]", " ", "xSols"}], ",", " ", 
