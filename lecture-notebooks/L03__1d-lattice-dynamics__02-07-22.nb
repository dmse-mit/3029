Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"xSols", ",", "pSols"}], "}"}], "=", 
   RowBox[{"NDSolveValue", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"momenta", "'"}], "[", "t", "]"}], "\[Equal]", 
        RowBox[{"harmonicForces", "[", 
         RowBox[{
          RowBox[{"positions", "[", "t", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "domainLength"}], "}"}]}], "]"}]}], ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"positions", "'"}], "[", "t", "]"}], " ", "\[Equal]", " ", 
        RowBox[{"momenta", "[", "t", "]"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"momenta", "[", "0", "]"}], " ", "==", " ", 
        "initialMomenta"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"positions", "[", "0", "]"}], "\[Equal]", 
        "initialPositions"}], ",", "\[IndentingNewLine]", 
       RowBox[{"WhenEvent", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Total", "[", 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               RowBox[{"positions", "[", "t", "]"}], "-", " ", 
               "initialPositions"}], ")"}], "2"], "]"}], " ", "<", 
