dab262b964cd"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"harmonicPotential", "[", "\[Rho]_", "]"}], "=", 
  RowBox[{"Normal", "[", 
   RowBox[{"Series", "[", 
    RowBox[{
     RowBox[{"lennardJonesPotentialNonDimensional", "[", "\[Rho]", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Rho]", ",", "1", ",", "2"}], "}"}]}], "]"}], 
   "]"}]}]], "Input",
