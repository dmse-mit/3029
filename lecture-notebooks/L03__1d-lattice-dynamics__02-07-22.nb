Cell[BoxData[
 RowBox[{
  RowBox[{"-", 
   FractionBox[
    RowBox[{"2", " ", "eMin", " ", 
     SuperscriptBox["rMin", "6"]}], 
    SuperscriptBox["r", "6"]]}], "+", 
  FractionBox[
   RowBox[{"eMin", " ", 
    SuperscriptBox["rMin", "12"]}], 
   SuperscriptBox["r", "12"]]}]], "Output",
 CellChangeTimes->{
  3.853235798549209*^9, {3.853235885068038*^9, 3.853235912123766*^9}},
