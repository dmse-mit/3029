   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Rho]", "\[LongEqual]", "1"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\rho=1", "state" -> "Boxes"|>,
