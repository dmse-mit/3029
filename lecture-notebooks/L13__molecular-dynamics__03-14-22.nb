          RowBox[{
            SuperscriptBox["\[PartialD]", "3"], 
            StyleBox["x", "TI"]}], 
          RowBox[{"3!", "\[PartialD]", 
            SuperscriptBox[
             StyleBox["t", "TI"], "3"]}]], "\[CapitalDelta]", 
         SuperscriptBox[
          StyleBox["t", "TI"], "3"], "+", 
