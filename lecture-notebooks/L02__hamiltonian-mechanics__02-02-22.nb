Cell[BoxData[{
 RowBox[{
  RowBox[{"moleculePotentialEnergyLJ", "=", 
   RowBox[{"Simplify", "[", 
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Rho]", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "[", "2", "]"}], "[", "t", "]"}], "-", 
         RowBox[{
          RowBox[{"x", "[", "1", "]"}], "[", "t", "]"}]}]}], "}"}], ",", 
      RowBox[{
       FractionBox["1", 
        SuperscriptBox["\[Rho]", "12"]], "-", 
       FractionBox["2", 
        SuperscriptBox["\[Rho]", "6"]]}]}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"moleculeLagrangianLJ", "=", 
   RowBox[{"Simplify", "[", 
    RowBox[{"moleculeKineticEnergy", "-", "moleculePotentialEnergyLJ"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"moleculeHamiltonianLJ", "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{"moleculeKineticEnergy", "+", "moleculePotentialEnergyLJ"}], 
     "]"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"moleculeSubstitutionsLJ", "=", 
    RowBox[{"First", "[", "\[IndentingNewLine]", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"p", "[", "1", "]"}], "[", "t", "]"}], "==", 
          RowBox[{"D", "[", 
           RowBox[{"moleculeLagrangianLJ", ",", 
            RowBox[{
             RowBox[{
              RowBox[{"x", "[", "1", "]"}], "'"}], "[", "t", "]"}]}], "]"}]}],
          ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"p", "[", "2", "]"}], "[", "t", "]"}], "==", 
          RowBox[{"D", "[", 
           RowBox[{"moleculeLagrangianLJ", ",", 
            RowBox[{
             RowBox[{
              RowBox[{"x", "[", "2", "]"}], "'"}], "[", "t", "]"}]}], 
           "]"}]}]}], "\[IndentingNewLine]", "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "1", "]"}], "'"}], "[", "t", "]"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"x", "[", "2", "]"}], "'"}], "[", "t", "]"}]}], "}"}]}], 
      "]"}], "\[IndentingNewLine]", "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"moleculeCanonicalHamiltonianLJ", "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{
      RowBox[{"moleculeHamiltonianLJ", "/.", "moleculeSubstitutionsLJ"}], "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", "[", "1", "]"}], "[", "t", "]"}], "->", 
         RowBox[{
          RowBox[{"q", "[", "1", "]"}], "[", "t", "]"}]}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"x", "[", "2", "]"}], "[", "t", "]"}], "->", 
         RowBox[{
          RowBox[{"q", "[", "2", "]"}], "[", "t", "]"}]}]}], "}"}]}], "]"}]}],
    ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"moleculeHamiltonsEquationsLJ", "=", " ", 
