          RowBox[{"p", "[", "1", "]"}], "[", "t", "]"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"q", "[", "2", "]"}], "[", "t", "]"}], ",", "t"}], "]"}], 
