Cell[BoxData[
 RowBox[{"\t", 
  TemplateBox[<|"boxes" -> FormBox[
      RowBox[{
        StyleBox["d", "TI"], 
        StyleBox["S", "TI"], "\[LongEqual]", 
        FractionBox["1", 
         StyleBox["T", "TI"]], 
        StyleBox["d", "TI"], 
        StyleBox["U", "TI"], "+", 
        FractionBox[
         StyleBox["P", "TI"], 
         StyleBox["V", "TI"]], 
        StyleBox["d", "TI"], 
        StyleBox["V", "TI"], "-", 
        UnderscriptBox["\[Sum]", 
         StyleBox["j", "TI"], LimitsPositioning -> True], 
        FractionBox[
         SubscriptBox["\[Mu]", 
          StyleBox["j", "TI"]], 
