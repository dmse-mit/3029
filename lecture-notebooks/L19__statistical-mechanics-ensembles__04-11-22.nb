Cell[BoxData[
 RowBox[{
  RowBox[{"stirlingsApproximation", "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"Log", "[", 
       RowBox[{"n_", "!"}], "]"}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"n", " ", 
        RowBox[{"Log", "[", "n", "]"}]}], "-", "n"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Log", "[", 
       RowBox[{"a_", "/", "b_"}], "]"}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"Log", "[", "a", "]"}], "-", 
       RowBox[{"Log", "[", "b", "]"}]}]}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Log", "[", 
       RowBox[{"a_", "*", "b_"}], "]"}], "\[RuleDelayed]", 
      RowBox[{
       RowBox[{"Log", "[", "a", "]"}], "+", 
       RowBox[{"Log", "[", "b", "]"}]}]}]}], "\[IndentingNewLine]", "}"}]}], 
  ";"}]], "Input",
 CellChangeTimes->{{3.858604363881715*^9, 3.8586043685372677`*^9}},
 CellLabel->"In[78]:=",ExpressionUUID->"5ce72805-5fea-44b4-b2a8-72ad528e59ec"],

