Cell[CellGroupData[{

Cell["Let\[CloseCurlyQuote]s \[OpenCurlyDoubleQuote]Brute-Force\
\[CloseCurlyDoubleQuote] it as a start", "Item",
 CellChangeTimes->{{3.858597227452862*^9, 
  3.858597245653853*^9}},ExpressionUUID->"599fdb96-6caa-44c4-a8cd-\
64755e9459ce"],

Cell[BoxData[
 RowBox[{
  RowBox[{"randomBallsNBars", "[", 
   RowBox[{"numberOfBalls_", ",", "numberOfBins_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"partitions", "=", 
       RowBox[{"RandomChoice", "[", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", "numberOfBalls"}], "]"}], ",", 
         RowBox[{"numberOfBins", "-", "1"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"balls", "=", 
       RowBox[{"Tuples", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Range", "[", "numberOfBalls", "]"}], ",", 
          RowBox[{"{", "0", "}"}]}], "}"}], "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"partitions", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Take", "[", 
          RowBox[{
           RowBox[{"Subdivide", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "-", 
              RowBox[{"1", "/", "4"}]}], ",", 
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "+", 
              RowBox[{"1", "/", "4"}]}], ",", 
             RowBox[{
              RowBox[{"Length", "[", "#", "]"}], "+", "1"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"2", ",", 
