Cell[CellGroupData[{

Cell[TextData[{
 "Fortunately, there\[CloseCurlyQuote]s a closed-form solution of the \
combinatorics above, given by the Binomial coefficient: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", GridBox[{
      {
       RowBox[{"U", "+", "N", "-", "1"}]},
      {"U"}
     }], ")"}], TraditionalForm]],ExpressionUUID->
  "0c04241d-d4d3-4a05-8c23-6eae7eeca9dd"]
}], "Item",
 CellChangeTimes->{{3.858598126367856*^9, 3.858598233489759*^9}, {
  3.858598427646966*^9, 
  3.8585984276470203`*^9}},ExpressionUUID->"a8908804-4d2b-4938-abfa-\
39e9e1330ea1"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"FunctionExpand", "[", 
  RowBox[{"Binomial", "[", 
   RowBox[{
    RowBox[{"U", "+", "N", "-", "1"}], ",", "U"}], "]"}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"%", "/.", 
  RowBox[{
   RowBox[{"Gamma", "[", "a_", "]"}], "\[RuleDelayed]", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"a", "-", "1"}], ")"}], "!"}]}]}]}], "Input",
 CellChangeTimes->{{3.858598239544485*^9, 3.858598243096901*^9}, 
   3.8585984294523573`*^9},
 CellLabel->"In[60]:=",ExpressionUUID->"7c2beaa5-d2d1-40a3-a17e-f5f1daa8468a"],

Cell[BoxData[
 FractionBox[
  RowBox[{"Gamma", "[", 
   RowBox[{"N", "+", "U"}], "]"}], 
  RowBox[{
   RowBox[{"Gamma", "[", "N", "]"}], " ", 
   RowBox[{"Gamma", "[", 
    RowBox[{"1", "+", "U"}], "]"}]}]]], "Output",
 CellChangeTimes->{3.8585982437564917`*^9, 3.858598430422318*^9},
 CellLabel->"Out[60]=",ExpressionUUID->"1c4dd90f-a3a6-4b6a-9a40-d4e977af463f"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "N", "+", "U"}], ")"}], "!"}], 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", "N"}], ")"}], "!"}], " ", 
   RowBox[{"U", "!"}]}]]], "Output",
 CellChangeTimes->{3.8585982437564917`*^9, 3.8585984304245996`*^9},
 CellLabel->"Out[61]=",ExpressionUUID->"abd35039-7061-486e-8bc8-2e0bff37143f"]
}, Open  ]]
