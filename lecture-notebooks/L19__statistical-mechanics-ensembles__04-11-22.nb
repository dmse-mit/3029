Cell[CellGroupData[{

Cell["\<\
As we saw before, Mathematica keeps Binomial coefficients using the \
generalized Gamma function,  \[CapitalGamma](n) = (n-1)!\
\>", "Item",
 CellChangeTimes->{{3.858598249842628*^9, 
  3.8585983153514013`*^9}},ExpressionUUID->"e2301ab0-40e2-4df2-801b-\
bb9af6c61e26"],

Cell[CellGroupData[{

Cell["So we\[CloseCurlyQuote]ll simply write out own", "Subitem",
 CellChangeTimes->{{3.858598249842628*^9, 
  3.858598323057616*^9}},ExpressionUUID->"80be105e-8850-4eb0-9645-\
391093a61a46"],

Cell[BoxData[
 RowBox[{
  RowBox[{"binomial", "[", 
   RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"ExpandAll", "[", 
   FractionBox[
    RowBox[{"n", "!"}], 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"n", "-", "k"}], ")"}], "!"}], 
     RowBox[{"k", "!"}]}]], "]"}]}]], "Input",
 CellLabel->"In[58]:=",ExpressionUUID->"edca59c5-5c8e-4feb-ab2d-2430e1a72e07"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"binomial", "[", 
  RowBox[{
   RowBox[{"U", "+", "N", "-", "1"}], ",", "U"}], "]"}]], "Input",
 CellChangeTimes->{3.8585983437873383`*^9, 3.8585984346094723`*^9},
 CellLabel->"In[62]:=",ExpressionUUID->"e6587b77-fcfb-47c9-96bb-5c62765a0bab"],

Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "1"}], "+", "N", "+", "U"}], ")"}], "!"}], 
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", "N"}], ")"}], "!"}], " ", 
   RowBox[{"U", "!"}]}]]], "Output",
 CellChangeTimes->{3.8585983442917547`*^9, 3.858598435263433*^9},
 CellLabel->"Out[62]=",ExpressionUUID->"b2949157-d030-4879-9fc2-eb84e8791b27"]
}, Open  ]]
}, Open  ]]
}, Open  ]],

