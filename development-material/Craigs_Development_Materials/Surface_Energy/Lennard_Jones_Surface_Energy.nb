       RowBox[{
        RowBox[{"strainRelaxedParticles", "[", "15", "]"}], " ", "+", 
        RowBox[{"(", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"0.2", " ", 
            RowBox[{"AngleVector", "[", "#", "]"}]}], "&"}], "/@", 
          RowBox[{"RandomReal", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"0", ",", 
