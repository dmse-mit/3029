       RowBox[{"Range", "[", 
        RowBox[{"Length", "[", "positions", "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"startingPoint", " ", "=", " ", 
      RowBox[{"MapThread", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"#1", ",", " ", "#2"}], "}"}], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"variables", ",", "positions"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"iter", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
