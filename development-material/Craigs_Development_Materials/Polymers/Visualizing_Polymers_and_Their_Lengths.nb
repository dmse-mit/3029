          RowBox[{"{", 
           RowBox[{"2", ",", "#"}], "}"}], "&"}], "/@", 
         RowBox[{"RandomReal", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"-", "range"}], "/", "2"}], ",", 
             RowBox[{"range", "/", "2"}]}], "}"}], ",", "12"}], "]"}]}], 
        "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Graphics", "[", "\[IndentingNewLine]", 
      InterpretationBox[
