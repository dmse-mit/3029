    RowBox[{
     RowBox[{"r", " ", "=", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"phi", "/", "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Flatten", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{"phi", "/", "4"}], "]"}], ",", 
        RowBox[{"r", " ", 
         RowBox[{"AngleVector", "[", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"2", "Pi"}]}], "}"}], "]"}], "]"}]}]}], "}"}], "]"}]}]}],
