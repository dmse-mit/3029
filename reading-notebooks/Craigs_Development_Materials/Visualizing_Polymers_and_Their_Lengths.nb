Cell[BoxData[
 RowBox[{
  RowBox[{"randomPointSphericalCap", "[", "\[Alpha]Max_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"phi", " ", "=", 
       RowBox[{"8", " ", 
        RowBox[{"ArcSin", "[", 
         RowBox[{
          RowBox[{"Sqrt", "[", 
           RowBox[{"RandomReal", "[", 
            RowBox[{"{", 
             RowBox[{"0", ",", "1"}], "}"}], "]"}], "]"}], 
          RowBox[{"Sin", "[", 
           FractionBox["\[Alpha]Max", "8"], "]"}]}], "]"}]}]}], ",", "r"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"r", " ", "=", " ", 
      RowBox[{"Sin", "[", 
       RowBox[{"phi", "/", "4"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Flatten", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Cos", "[", 
         RowBox[{"phi", "/", "4"}], "]"}], ",", 
        RowBox[{"r", " ", 
         RowBox[{"AngleVector", "[", 
          RowBox[{"RandomReal", "[", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"2", "Pi"}]}], "}"}], "]"}], "]"}]}]}], "}"}], "]"}]}]}],
    "\[IndentingNewLine]", "]"}]}]], "Input",
