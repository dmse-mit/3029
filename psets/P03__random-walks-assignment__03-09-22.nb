 RowBox[{
  RowBox[{
   RowBox[{"checkForCollisions", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"listOfPositions_", ",", "oldListOfPositions_"}], "}"}], ",", 
     RowBox[{"forbidLoops_", ":", "True"}]}], "]"}], ":=", 
   RowBox[{"Catch", "[", "\[IndentingNewLine]", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"listOfPos", "=", "listOfPositions"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"bbs", "=", 
         RowBox[{"axesAlignedBoundingBoxes", "/@", "listOfPositions"}]}], ",",
         "\[IndentingNewLine]", "mij", ",", "nonzero", ",", "collisions", ",",
         "collisionIndices", ",", "endpointChecks", ",", "lengths"}], "}"}], 
      ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"mij", "=", 
        RowBox[{"connectivityMatrix", "[", "bbs", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"mij", "[", "\"\<ExplicitLength\>\"", "]"}], "==", "0"}], 
         ",", 
         RowBox[{
          RowBox[{"Echo", "[", "\"\<No Bounding Box Collisions\>\"", "]"}], 
          ";", 
          RowBox[{"Return", "[", "listOfPos", "]"}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"nonzero", "=", 
        RowBox[{"mij", "[", "\"\<NonzeroPositions\>\"", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"collisions", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"pairwiseChainCollisions", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"listOfPos", ",", "bbs"}], "}"}], ",", "#"}], "]"}], 
          "&"}], "/@", "nonzero"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", 
