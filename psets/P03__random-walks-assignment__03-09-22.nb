Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"greatCircleDistance", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x1_", ",", "y1_", ",", "z1_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x2_", ",", "y2_", ",", "z2_"}], "}"}], ",", 
